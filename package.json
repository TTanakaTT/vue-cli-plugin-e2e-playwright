{
  "name": "vue-cli-plugin-e2e-playwright",
  "version": "0.3.5",
  "description": "Vue CLI plugin for e2e testing with Playwright",
  "keywords": [
    "playwright",
    "vue",
    "vuejs",
    "vue-cli",
    "vue/cli-plugin",
    "e2e",
    "e2e-playwright",
    "plugin"
  ],
  "homepage": "https://github.com/marcol/vue-cli-plugin-e2e-playwright",
  "bugs": "https://github.com/marcol/vue-cli-plugin-e2e-playwright/issues",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcol/vue-cli-plugin-e2e-playwright.git"
  },
  "author": "Marco Dias Lopes <rhino@pacodacomenda.com>",
  "scripts": {
    "lint": "yarn lint:js; yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark .",
    "test": "mocha __tests__/*.js"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "playwright": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "remark-cli": "^8.0.1",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-markdown-style-guide": "^3.0.1"
  },
  "husky": {
    "husky": {
      "hooks": {
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
        "pre-push": "yarn test",
        "pre-commit": "yarn lint"
      }
    }
  }
}
