{
  "name": "vue-cli-plugin-e2e-playwright",
  "version": "1.0.0",
  "description": "Vue CLI plugin for e2e testing with Playwright",
  "private": false,
  "keywords": [
    "playwright",
    "vue",
    "vuejs",
    "vue-cli",
    "vue/cli-plugin",
    "e2e",
    "tests",
    "end to end",
    "e2e-playwright",
    "plugin"
  ],
  "homepage": "https://github.com/marcol/vue-cli-plugin-e2e-playwright",
  "bugs": "https://github.com/marcol/vue-cli-plugin-e2e-playwright/issues",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcol/vue-cli-plugin-e2e-playwright.git"
  },
  "author": "Marco Dias Lopes <rhino@pacodacomenda.com>",
  "scripts": {
    "lint": "yarn lint:js; yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark .",
    "test": "mocha __tests__/*.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "chai": "^4.3.0",
    "mocha": "^8.3.0",
    "playwright": "^1.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.1.1",
    "pinst": "^2.1.6",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0"
  }
}
